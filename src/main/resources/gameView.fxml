<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<VBox prefHeight="278.0" prefWidth="267.0" xmlns="http://javafx.com/javafx/11.0.1"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="GamePresenter">

  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="379.0" prefWidth="390.0"
      VBox.vgrow="ALWAYS">
      <children>
        <Text fx:id="textField" layoutX="6.0" layoutY="260.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="............. wins" wrappingWidth="255.13671875"/>
        <GridPane fx:id="gridPane" layoutY="28.0" prefHeight="198.0" prefWidth="267.0">
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
          </rowConstraints>
          <children>
            <ImageView fx:id="f00" fitHeight="65.0" fitWidth="88.0" pickOnBounds="true"
              preserveRatio="true" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
            <ImageView fx:id="f01" fitHeight="65.0" fitWidth="88.0" pickOnBounds="true"
              preserveRatio="true" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
            <ImageView fx:id="f02" fitHeight="65.0" fitWidth="88.0" pickOnBounds="true"
              preserveRatio="true" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
            <ImageView fx:id="f10" fitHeight="65.0" fitWidth="88.0" pickOnBounds="true"
              preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
            <ImageView fx:id="f11" fitHeight="65.0" fitWidth="88.0" pickOnBounds="true"
              preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <ImageView fx:id="f12" fitHeight="65.0" fitWidth="88.0" pickOnBounds="true"
              preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            <ImageView fx:id="f20" fitHeight="65.0" fitWidth="88.0" pickOnBounds="true"
              preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
            <ImageView fx:id="f21" fitHeight="65.0" fitWidth="88.0" pickOnBounds="true"
              preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
            <ImageView fx:id="f22" fitHeight="65.0" fitWidth="88.0" pickOnBounds="true"
              preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
          </children>
        </GridPane>
        <ToggleButton fx:id="aiToggle" layoutX="169.0" layoutY="2.0" mnemonicParsing="false"
          onMouseClicked="#toggleAi" text="Play against AI"/>
        <Button fx:id="ngButton" layoutY="2.0" mnemonicParsing="false" onMouseClicked="#newGame"
          text="New Game"/>
      </children>
    </AnchorPane>
  </children>
</VBox>
